<%- include('./partials/nav') %>

    <!DOCTYPE html>
    <html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>category</title>
    </head>

    <body>
        <h1>Category: <%= category %>
        </h1>
        <ul>
            <% menuItems.forEach(item=> { %>
                <li>
                    <h2>
                        <%= item.name %>
                    </h2>
                    <p>
                        <%= item.details %>
                    </p>
                    <p>Price: <%= item.price%>
                    </p>
                    <p>Rating: <%= item.rating %> stars</p>
                </li>
                <% }) %>
        </ul>

    </body>

    </html>